@inherits StockList
@attribute [Authorize]


@((RenderFragment)base.BuildRenderTree)

@code
{
    protected override async Task GetStocksAsync()
    {
        Stocks = await StocksService.GetWatchlistAsync();
        StocksToShow = Stocks;
    }

    protected override Task GetStocksToShowAsync()
    {
        return Task.CompletedTask;
    }
}